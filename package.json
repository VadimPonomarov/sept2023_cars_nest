{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "kill": "netstat -ano | findstr :3000",
    "kill:sid": "tskill",
    "format": "prettier --write \"src/**/*.ts\"",
    "build": "nest build && tsc",
    "start": "nest start --watch",
    "start:debug": "npm run build && node dist/main --debug --watch",
    "start:dev": "node dist/main",
    "start:prod": "node dist/main --env-file .env ",
    "stop": "kill-port 3000",
    "secret:generate": "node -e \"console.log( require('crypto').randomBytes(24).toString('base64url'))\"",
    "restart": "npm rum stop && npm run start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "check:updates": "ncu -u",
    "typeorm": "typeorm-ts-node-commonjs --dataSource src/modules/db/migrations.connect.ts",
    "migration:create": "cross-var npm run typeorm -- migration:create ./src/modules/db/migrations/$npm_config_name",
    "migration:generate": "npm run build && cross-var npm run typeorm -- migration:generate ./src/modules/db/migrations/$npm_config_name",
    "migration:revert": "npm run typeorm -- migration:revert && npm run build",
    "migration:run": "npm run build && npm run typeorm -- migration:run"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.600.0",
    "@google/maps": "^1.1.3",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/typeorm": "^10.0.2",
    "@sendgrid/mail": "^8.1.3",
    "@sentry/cli": "^2.32.1",
    "@sentry/node": "^8.11.0",
    "@sentry/profiling-node": "^8.11.0",
    "@sentry/webpack-plugin": "^2.19.0",
    "@types/lodash": "^4.17.6",
    "@types/morgan": "^1.9.9",
    "@types/pino": "^7.0.5",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cross-var": "^1.1.0",
    "g4f": "^1.4.6",
    "lodash-es": "^4.17.21",
    "moment": "^2.30.1",
    "nest-winston": "^1.10.0",
    "nestjs-pino": "^4.1.0",
    "nodemon": "^3.1.1",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.5",
    "pino-http": "^10.1.0",
    "pino-pretty": "^11.1.0",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "ts-transformer-keys": "^0.4.4",
    "typeorm": "^0.3.20",
    "utility-types": "^3.11.0",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.17",
    "@types/google.maps": "^3.55.11",
    "@types/googlemaps": "^3.43.3",
    "@types/jest": "^29.5.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "^4.0.1",
    "@types/passport-local": "^1.0.38",
    "@types/supertest": "^6.0.0",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "google-maps-api-typings": "^1.0.5",
    "husky": "^9.0.11",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.7",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "wait-port": "^1.1.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^my_app/my-lib(|/.*)$": "<rootDir>/libs/my-lib/src/$1"
    }
  }
}
